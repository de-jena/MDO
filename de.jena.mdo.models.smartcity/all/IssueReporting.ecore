    <eLiterals name="InProgress" value="2"/>
    <eLiterals name="Open" value="3"/>
    <eLiterals name="Unassigned" value="4"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EEnum" name="IssueReportingTypeValue">
    <eLiterals name="IssueReporting"/>
