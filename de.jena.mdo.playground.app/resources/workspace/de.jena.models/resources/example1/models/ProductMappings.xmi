<?xml version="1.0" encoding="UTF-8"?>
<eorm:EntityMappings
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore"
    xmlns:eorm="https://eclipse.org/fennec/persistence/eorm/1.0.0"
    schema="PRODUCT"
    version="3.1"
    name="example1">
  <entity name="product">
    <table name="PRODUCT"/>
    <attributes>
      <id name="id">
        <feature
            xsi:type="ecore:EAttribute"
            href="product.ecore#//Product/id"/>
        <column
            columnDefinition=""
            name="PRODUCT_ID"
            nullable="false"
            unique="true"/>
        <sequenceGenerator
            name="SEQ_PRODUCT_ID_ID"
            sequenceName="SEQ_PRODUCT_ID_ID"/>
      </id>
      <basic
          name="product_name2"
          fetch="EAGER">
        <feature
            xsi:type="ecore:EAttribute"
            href="product.ecore#//Product/name"/>
        <column
            name="product_name"/>
      </basic>
      <basic
          name="brand_name"
          fetch="EAGER">
        <feature
            xsi:type="ecore:EAttribute"
            href="product.ecore#//Product/brand2"/>
        <column
            name="brand_name"
            table=""/>
      </basic>
      <basic
          name="ID"
          fetch="EAGER">
        <feature
            xsi:type="ecore:EAttribute"
            href="product.ecore#//Product/id"/>
        <column
            name="PRODUCT_ID"
            length="30"/>
      </basic>
    </attributes>
    <class xsi:type="ecore:EClass"
        href="product.ecore#//Product"/>
  </entity>
  <entity name="productClass">
    <table name="PRODUCT_CLASS"/>
    <attributes>
      <id name="id">
        <feature
            xsi:type="ecore:EAttribute"
            href="product.ecore#//ProductClass/id"/>
        <column
            name="PRODUCT_CLASS_ID"
            unique="true"/>
        <sequenceGenerator
            name="SEQ_PRODUCT_CLASS_ID_ID"
            sequenceName="SEQ_PRODUCT_CLASS_ID_ID"/>
      </id>
      <basic
          name="PRODUCT_CATEGORY"
          fetch="EAGER">
        <feature
            xsi:type="ecore:EAttribute"
            href="product.ecore#//ProductClass/category"/>
        <column
            name="PRODUCT_CATEGORY"
            length="30"/>
      </basic>
      <basic
          name="PRODUCT_DEPARTMENT"
          fetch="EAGER">
        <feature
            xsi:type="ecore:EAttribute"
            href="product.ecore#//ProductClass/departement"/>
        <column
            name="PRODUCT_DEPARTMENT"
            length="30"/>
      </basic>
      <basic
          name="PRODUCT_FAMILE"
          fetch="EAGER">
        <feature
            xsi:type="ecore:EAttribute"
            href="product.ecore#//ProductClass/family"/>
        <column
            name="PRODUCT_FAMILE"
            length="30"/>
      </basic>
      <basic
          name="ID"
          fetch="EAGER">
        <feature
            xsi:type="ecore:EAttribute"
            href="product.ecore#//ProductClass/id"/>
        <column
            name="PRODUCT_CLASS_ID"
            length="30"/>
      </basic>
    </attributes>
    <class xsi:type="ecore:EClass"
        href="product.ecore#//ProductClass"/>
  </entity>
</eorm:EntityMappings>
